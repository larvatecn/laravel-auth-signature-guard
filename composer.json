{
    "name": "larva/laravel-auth-signature-guard",
    "description": "This is a Laravel auth guard for the signature.",
    "keywords": [
        "auth",
        "laravel",
        "signature"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "laravel/passport": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/support": "^5.1 || ^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Larva\\Auth\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Larva\\Auth\\AuthServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
